nt.email && newClient.phone && newClient.address) {\n        this.clients.push(newClient);\n        alert(\"New client added successfully!\");\n      } else {\n        alert(\"Please fill in all the details!\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","clients","id","name","email","phone","lastVisit","address","isModalOpen","isEditing","selectedClient","methods","viewDetails","client","toggleEdit","saveClient","index","findIndex","splice","closeModal","addClient","newClient","length","prompt","Date","toISOString","split","push","alert"],"sources":["C:\\Users\\HP-15\\Desktop\\parlouradmin\\parlouradmin\\src\\components\\ClientPage.vue"],"sourcesContent":["<template>\r\n    <div class=\"client-page\">\r\n      <div class=\"overlay\">\r\n        <h1>Clients</h1>\r\n        <div class=\"client-list\">\r\n          <div v-for=\"client in clients\" :key=\"client.id\" class=\"client-card\">\r\n            <div class=\"card-content\">\r\n              <div class=\"client-info\">\r\n                <h2 class=\"client-name\">{{ client.name }}</h2>\r\n                <p class=\"client-email\">{{ client.email }}</p>\r\n                <p><strong>Phone:</strong> {{ client.phone }}</p>\r\n                <p><strong>Last Visit:</strong> {{ client.lastVisit }}</p>\r\n                <button @click=\"viewDetails(client)\" class=\"view-details-btn\">View Details</button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <button @click=\"addClient\" class=\"add-client-btn\">Add New Client</button>\r\n      </div>\r\n  \r\n      <!-- Modal for Full Details / Edit Client -->\r\n      <div v-if=\"isModalOpen\" class=\"modal-overlay\" @click=\"closeModal\">\r\n        <div class=\"modal-content\" @click.stop>\r\n          <h2 class=\"modal-title\">{{ isEditing ? 'Edit' : 'View' }} Client: {{ selectedClient.name }}</h2>\r\n  \r\n          <div v-if=\"isEditing\">\r\n            <label for=\"clientName\">Name:</label>\r\n            <input type=\"text\" v-model=\"selectedClient.name\" id=\"clientName\" />\r\n  \r\n            <label for=\"clientEmail\">Email:</label>\r\n            <input type=\"email\" v-model=\"selectedClient.email\" id=\"clientEmail\" />\r\n  \r\n            <label for=\"clientPhone\">Phone:</label>\r\n            <input type=\"text\" v-model=\"selectedClient.phone\" id=\"clientPhone\" />\r\n  \r\n            <label for=\"clientLastVisit\">Last Visit:</label>\r\n            <input type=\"date\" v-model=\"selectedClient.lastVisit\" id=\"clientLastVisit\" />\r\n  \r\n            <label for=\"clientAddress\">Address:</label>\r\n            <input type=\"text\" v-model=\"selectedClient.address\" id=\"clientAddress\" />\r\n  \r\n            <button @click=\"saveClient\" class=\"save-btn\">Save Changes</button>\r\n          </div>\r\n  \r\n          <div v-else>\r\n            <p><strong>Email:</strong> {{ selectedClient.email }}</p>\r\n            <p><strong>Phone:</strong> {{ selectedClient.phone }}</p>\r\n            <p><strong>Last Visit:</strong> {{ selectedClient.lastVisit }}</p>\r\n            <p><strong>Address:</strong> {{ selectedClient.address }}</p>\r\n            <button @click=\"toggleEdit\" class=\"edit-btn\">Edit Client</button>\r\n          </div>\r\n  \r\n          <button @click=\"closeModal\" class=\"close-btn\">Close</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        clients: [\r\n          { id: 1, name: 'John Doe', email: 'john@example.com', phone: '123-456-7890', lastVisit: '2024-12-15', address: '123 Main St, Springfield' },\r\n          { id: 2, name: 'Jane Smith', email: 'jane@example.com', phone: '098-765-4321', lastVisit: '2024-11-30', address: '456 Oak St, Springfield' },\r\n          { id: 3, name: 'Alice Johnson', email: 'alice@example.com', phone: '567-890-1234', lastVisit: '2024-10-20', address: '789 Pine St, Springfield' }\r\n        ],\r\n        isModalOpen: false,\r\n        isEditing: false,\r\n        selectedClient: null\r\n      };\r\n    },\r\n    methods: {\r\n      viewDetails(client) {\r\n        this.selectedClient = { ...client };  // Clone client data to avoid direct mutation\r\n        this.isEditing = false;\r\n        this.isModalOpen = true;\r\n      },\r\n      toggleEdit() {\r\n        this.isEditing = !this.isEditing;  // Toggle between view and edit mode\r\n      },\r\n      saveClient() {\r\n        // Save changes to the client\r\n        const index = this.clients.findIndex(client => client.id === this.selectedClient.id);\r\n        if (index !== -1) {\r\n          this.clients.splice(index, 1, { ...this.selectedClient });\r\n        }\r\n        this.closeModal();  // Close modal after saving changes\r\n      },\r\n      closeModal() {\r\n        this.isModalOpen = false;\r\n        this.selectedClient = null;\r\n      },\r\n      addClient() {\r\n        const newClient = {\r\n          id: this.clients.length + 1,\r\n          name: prompt(\"Enter client's name:\"),\r\n          email: prompt(\"Enter client's email:\"),\r\n          phone: prompt(\"Enter client's phone number:\"),\r\n          lastVisit: new Date().toISOString().split('T')[0],\r\n          address: prompt(\"Enter client's address:\")\r\n        };\r\n        if (newClient.name && newClient.email && newClient.phone && newClient.address) {\r\n          this.clients.push(newClient);\r\n          alert(\"New client added successfully!\");\r\n        } else {\r\n          alert(\"Please fill in all the details!\");\r\n        }\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .client-page {\r\n    background-image: url('@/assets/salon.jpg');\r\n    background-size: cover;\r\n    background-position: center;\r\n    height: 100vh;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    flex-direction: column;\r\n    padding: 40px;\r\n    color: white;\r\n  }\r\n  \r\n  .overlay {\r\n    background: rgba(0, 0, 0, 0.6);\r\n    width: 100%;\r\n    max-width: 1200px;\r\n    border-radius: 10px;\r\n    padding: 20px;\r\n    text-align: center;\r\n  }\r\n  \r\n  h1 {\r\n    font-size: 3rem;\r\n    color: gold;\r\n    margin-bottom: 20px;\r\n    font-family: 'Arial', sans-serif;\r\n  }\r\n  \r\n  .client-list {\r\n    display: grid;\r\n    grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));\r\n    gap: 20px;\r\n    margin-bottom: 30px;\r\n  }\r\n  \r\n  .client-card {\r\n    background-color: rgba(0, 0, 0, 0.8);\r\n    border-radius: 8px;\r\n    overflow: hidden;\r\n    box-shadow: 0 10px 20px rgba(0, 0, 0, 0.3);\r\n  }\r\n  \r\n  .card-content {\r\n    padding: 20px;\r\n  }\r\n  \r\n  .client-name {\r\n    font-size: 1.6rem;\r\n    color: gold;\r\n    margin-bottom: 10px;\r\n  }\r\n  \r\n  .client-email {\r\n    color: #f5c518;\r\n    font-size: 1.2rem;\r\n    margin-bottom: 10px;\r\n  }\r\n  \r\n  .client-info p {\r\n    font-size: 1rem;\r\n    color: white;\r\n    margin: 5px 0;\r\n  }\r\n  \r\n  .view-details-btn {\r\n    background-color: gold;\r\n    color: black;\r\n    padding: 10px 20px;\r\n    font-weight: bold;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n    transition: 0.3s;\r\n  }\r\n  \r\n  .view-details-btn:hover {\r\n    background-color: black;\r\n    color: gold;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.4);\r\n  }\r\n  \r\n  .add-client-btn {\r\n    background-color: gold;\r\n    color: black;\r\n    padding: 15px 30px;\r\n    font-size: 1.2rem;\r\n    border: none;\r\n    border-radius: 50px;\r\n    font-weight: bold;\r\n    cursor: pointer;\r\n    transition: 0.3s;\r\n    margin-top: 20px;\r\n  }\r\n  \r\n  .add-client-btn:hover {\r\n    background-color: black;\r\n    color: gold;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.4);\r\n  }\r\n  \r\n  /* Modal Styles */\r\n  .modal-overlay {\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    background: rgba(0, 0, 0, 0.7);\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n  \r\n  .modal-content {\r\n    background-color: white;\r\n    color: black;\r\n    padding: 30px;\r\n    border-radius: 8px;\r\n    width: 80%;\r\n    max-width: 800px;\r\n    text-align: left;\r\n    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.5);\r\n  }\r\n  \r\n  .modal-title {\r\n    font-size: 2rem;\r\n    color: gold;\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  input {\r\n    width: 100%;\r\n    padding: 10px;\r\n    margin: 10px 0;\r\n    border-radius: 5px;\r\n    border: 1px solid #ccc;\r\n  }\r\n  \r\n  .save-btn {\r\n    background-color: gold;\r\n    color: black;\r\n    padding: 10px 20px;\r\n    font-weight: bold;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .save-btn:hover {\r\n    background-color: black;\r\n    color: gold;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.4);\r\n  }\r\n  \r\n  .edit-btn {\r\n    background-color: gold;\r\n    color: black;\r\n    padding: 10px 20px;\r\n    font-weight: bold;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n    transition: 0.3s;\r\n  }\r\n  \r\n  .edit-btn:hover {\r\n    background-color: black;\r\n    color: gold;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.4);\r\n  }\r\n  \r\n  .close-btn {\r\n    background-color: gold;\r\n    color: black;\r\n    padding: 10px 20px;\r\n    font-weight: bold;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n    margin-top: 10px;\r\n  }\r\n  \r\n  .close-btn:hover {\r\n    background-color: black;\r\n    color: gold;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.4);\r\n  }\r\n  </style>\r\n  "],"mappings":";AA2DE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,CACP;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,UAAU;QAAEC,KAAK,EAAE,kBAAkB;QAAEC,KAAK,EAAE,cAAc;QAAEC,SAAS,EAAE,YAAY;QAAEC,OAAO,EAAE;MAA2B,CAAC,EAC3I;QAAEL,