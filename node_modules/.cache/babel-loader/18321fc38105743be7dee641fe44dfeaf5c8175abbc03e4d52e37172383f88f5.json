{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      staff: {\n        name: '',\n        role: '',\n        email: ''\n      },\n      staffList: [],\n      isEditing: false,\n      // Tracks if we're editing a staff member\n      editIndex: null // Stores the index of the staff being edited\n    };\n  },\n  methods: {\n    handleSubmit() {\n      if (this.isEditing) {\n        // Save changes to the edited staff\n        this.staffList[this.editIndex] = {\n          ...this.staff\n        };\n        this.resetForm();\n      } else {\n        // Add new staff\n        this.staffList.push({\n          ...this.staff\n        });\n        this.resetForm();\n      }\n    },\n    editStaff(index) {\n      // Load the staff data into the form for editing\n      this.staff = {\n        ...this.staffList[index]\n      };\n      this.isEditing = true;\n      this.editIndex = index;\n    },\n    removeStaff(index) {\n      // Remove the staff from the list\n      this.staffList.splice(index, 1);\n    },\n    resetForm() {\n      // Reset the form and variables\n      this.staff = {\n        name: '',\n        role: '',\n        email: ''\n      };\n      this.isEditing = false;\n      this.editIndex = null;\n    }\n  }\n};","map":{"version":3,"names":["data","staff","name","role","email","staffList","isEditing","editIndex","methods","handleSubmit","resetForm","push","editStaff","index","removeStaff","splice"],"sources":["C:\\Users\\HP-15\\Desktop\\parlouradmin\\parlouradmin\\src\\components\\ManageStaff.vue"],"sourcesContent":["<template>\r\n    <div class=\"staff-management\">\r\n      <div class=\"container\">\r\n        <h1 class=\"title\">Staff Management</h1>\r\n  \r\n        <!-- Staff Form -->\r\n        <form @submit.prevent=\"handleSubmit\" class=\"staff-form\">\r\n          <input\r\n            v-model=\"staff.name\"\r\n            type=\"text\"\r\n            placeholder=\"Staff Name\"\r\n            required\r\n            class=\"input-field\"\r\n          />\r\n          <input\r\n            v-model=\"staff.role\"\r\n            type=\"text\"\r\n            placeholder=\"Role\"\r\n            required\r\n            class=\"input-field\"\r\n          />\r\n          <input\r\n            v-model=\"staff.email\"\r\n            type=\"email\"\r\n            placeholder=\"Email\"\r\n            required\r\n            class=\"input-field\"\r\n          />\r\n          <button type=\"submit\" class=\"submit-btn\">\r\n            {{ isEditing ? \"Save Changes\" : \"Add Staff\" }}\r\n          </button>\r\n        </form>\r\n  \r\n        <!-- Staff List Table -->\r\n        <div class=\"staff-table\">\r\n          <table>\r\n            <thead>\r\n              <tr>\r\n                <th>Name</th>\r\n                <th>Role</th>\r\n                <th>Email</th>\r\n                <th>Actions</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"(staff, index) in staffList\" :key=\"index\">\r\n                <td>{{ staff.name }}</td>\r\n                <td>{{ staff.role }}</td>\r\n                <td>{{ staff.email }}</td>\r\n                <td class=\"actions\">\r\n                  <button @click=\"editStaff(index)\">✏️</button>\r\n                  <button @click=\"removeStaff(index)\">❌</button>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        staff: {\r\n          name: '',\r\n          role: '',\r\n          email: '',\r\n        },\r\n        staffList: [],\r\n        isEditing: false, // Tracks if we're editing a staff member\r\n        editIndex: null, // Stores the index of the staff being edited\r\n      };\r\n    },\r\n    methods: {\r\n      handleSubmit() {\r\n        if (this.isEditing) {\r\n          // Save changes to the edited staff\r\n          this.staffList[this.editIndex] = { ...this.staff };\r\n          this.resetForm();\r\n        } else {\r\n          // Add new staff\r\n          this.staffList.push({ ...this.staff });\r\n          this.resetForm();\r\n        }\r\n      },\r\n      editStaff(index) {\r\n        // Load the staff data into the form for editing\r\n        this.staff = { ...this.staffList[index] };\r\n        this.isEditing = true;\r\n        this.editIndex = index;\r\n      },\r\n      removeStaff(index) {\r\n        // Remove the staff from the list\r\n        this.staffList.splice(index, 1);\r\n      },\r\n      resetForm() {\r\n        // Reset the form and variables\r\n        this.staff = { name: '', role: '', email: '' };\r\n        this.isEditing = false;\r\n        this.editIndex = null;\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .staff-management {\r\n    background-image: url('@/assets/salon.jpg');\r\n    background-size: cover;\r\n    background-position: center;\r\n    min-height: 100vh;\r\n    padding: 20px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n  \r\n  .container {\r\n    background: rgba(255, 255, 255, 0.8);\r\n    padding: 30px;\r\n    border-radius: 8px;\r\n    width: 100%;\r\n    max-width: 800px;\r\n    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n  }\r\n  \r\n  .title {\r\n    text-align: center;\r\n    font-size: 2em;\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  .staff-form {\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap: 15px;\r\n  }\r\n  \r\n  .input-field {\r\n    padding: 10px;\r\n    border-radius: 5px;\r\n    border: 1px solid #ccc;\r\n    font-size: 1em;\r\n  }\r\n  \r\n  .submit-btn {\r\n    padding: 10px;\r\n    background-color: #4caf50;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .submit-btn:hover {\r\n    background-color: #45a049;