          <th>Status</th>\r\n            <th>Actions</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"appointment in filteredAppointments\" :key=\"appointment.id\">\r\n            <td>{{ appointment.customerName }}</td>\r\n            <td>{{ appointment.date }}</td>\r\n            <td>{{ appointment.time }}</td>\r\n            <td>{{ appointment.service }}</td>\r\n            <td>\r\n              <span :class=\"'status ' + getStatusClass(appointment.status)\">\r\n                {{ appointment.status }}\r\n              </span>\r\n            </td>\r\n            <td>\r\n              <button class=\"btn-icon btn-edit\" @click=\"editAppointment(appointment)\">\r\n                ‚úèÔ∏è\r\n              </button>\r\n              <button class=\"btn-icon btn-delete\" @click=\"deleteAppointment(appointment.id)\">\r\n                üóëÔ∏è\r\n              </button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        appointments: [],\r\n        filteredAppointments: [],\r\n        search: '',\r\n        selectedStatus: '',\r\n        statuses: ['Confirmed', 'Pending', 'Cancelled', 'Completed'],\r\n      };\r\n    },\r\n    methods: {\r\n      fetchAppointments() {\r\n        this.appointments = [\r\n          {\r\n            id: 1,\r\n            customerName: 'Alice Johnson',\r\n            date: '2024-12-01',\r\n            time: '10:00 AM',\r\n            service: 'Haircut',\r\n            status: 'Confirmed',\r\n          },\r\n          {\r\n            id: 2,\r\n            customerName: 'Bob Smith',\r\n            date: '2024-12-02',\r\n            time: '2:00 PM',\r\n            service: 'Facial',\r\n            status: 'Pending',\r\n          },\r\n          {\r\n            id: 3,\r\n            customerName: 'Sarah Lee',\r\n            date: '2024-12-03',\r\n            time: '4:00 PM',\r\n            service: 'Massage',\r\n            status: 'Completed',\r\n          },\r\n        ];\r\n        this.filteredAppointments = [...this.appointments];\r\n      },\r\n      applyFilters() {\r\n        this.filteredAppointments = this.appointments.filter((appointment) => {\r\n          const matchesSearch =\r\n            !this.search ||\r\n            appointment.customerName\r\n              .toLowerCase()\r\n              .includes(this.search.toLowerCase());\r\n          const matchesStatus =\r\n            !this.selectedStatus || appointment.status === this.selectedStatus;\r\n          return matchesSearch && matchesStatus;\r\n        });\r\n      },\r\n      getStatusClass(status) {\r\n        switch (status) {\r\n          case 'Confirmed':\r\n            return 'status-confirmed';\r\n          case 'Pending':\r\n            return 'status-pending';\r\n          case 'Cancelled':\r\n            return 'status-cancelled';\r\n          case 'Completed':\r\n            return 'status-completed';\r\n          default:\r\n            return '';\r\n        }\r\n      },\r\n      editAppointment(appointment) {\r\n        console.log('Edit appointment', appointment);\r\n      },\r\n      deleteAppointment(appointmentId) {\r\n        this.appointments = this.appointments.filter(\r\n          (appointment) => appointment.id !== appointmentId\r\n        );\r\n        this.applyFilters();\r\n        console.log('Deleted appointment', appointmentId);\r\n      },\r\n    },\r\n    mounted() {\r\n      this.fetchAppointments();\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n/* Background Styling */\r\n.container {\r\n  background-image: url('/assets/salon.jpg'); /* Replace with your actual path */\r\n  background-size: cover;\r\n  background-position: center;\r\n  padding: 20px;\r\n  border-radius: 10px;\r\n  backdrop-filter: blur(5px);\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);\r\n}\r\n\r\n/* Page Title Styling */\r\n.page-title {\r\n  text-align: center;\r\n  font-size: 28px;\r\n  font-weight: bold;\r\n  color: #fff;\r\n  margin-bottom: 30px;\r\n  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.6);\r\n}\r\n\r\n/* Filters Section */\r\n.filters {\r\n  display: flex;\r\n  gap: 10px;\r\n  flex-wrap: wrap;\r\n  background-color: rgba(255, 255, 255, 0.8);\r\n  padding: 15px;\r\n  border-radius: 8px;\r\n}\r\n\r\n.filter-input,\r\n.filter-select {\r\n  width: 100%;\r\n  padding: 8px;\r\n  font-size: 16px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px;\r\n}\r\n\r\n.btn-primary {\r\n  padding: 10px 15px;\r\n  background-color: #e83e8c;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  font-weight: bold;\r\n}\r\n\r\n.btn-primary:hover {\r\n  background-color: #d63384;\r\n}\r\n\r\n/* Appointment Table */\r\n.appointment-table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  margin-top: 20px;\r\n  background-color: rgba(255, 255, 255, 0.9);\r\n  border-radius: 10px;\r\n  overflow: hidden;\r\n}\r\n\r\n.appointment-table th,\r\n.appointment-table td {\r\n  padding: 12px;\r\n  text-align: left;\r\n  border: 1px solid #ddd;\r\n  color: #000; /* Set font color to black */\r\n}\r\n\r\n.appointment-table th {\r\n  background-color: #f4f4f4;\r\n  font-weight: bold;\r\n  color: #000; /* Ensure header font is also black */\r\n}\r\n\r\n.status {\r\n  padding: 5px 10px;\r\n  border-radius: 5px;\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n  display: inline-block;\r\n}\r\n\r\n.status-confirmed {\r\n  background-color: #28a745;\r\n  color: white;\r\n}\r\n\r\n.status-pending {\r\n  background-color: #ffc107;\r\n  color: white;\r\n}\r\n\r\n.status-cancelled {\r\n  background-color: #dc3545;\r\n  color: white;\r\n}\r\n\r\n.status-completed {\r\n  background-color: #17a2b8;\r\n  color: white;\r\n}\r\n\r\n/* Buttons */\r\n.btn-icon {\r\n  background: none;\r\n  border: none;\r\n  cursor: pointer;\r\n  font-size: 18px;\r\n  margin: 0 5px;\r\n}\r\n\r\n.btn-icon:hover {\r\n  opacity: 0.8;\r\n}\r\n\r\n.btn-edit {\r\n  color: green;\r\n}\r\n\r\n.btn-delete {\r\n  color: red;\r\n}\r\n</style>\r\n\r\n  "],"mappings":";;EACSA,KAAK,EAAC;AAAW;;EAIfA,KAAK,EAAC;AAAc;;EAClBA,KAAK,EAAC;AAAc;;EAQpBA,KAAK,EAAC;AAAc;mBAdjC;;EA0BaA,KAAK,EAAC;AAAmB;mBA1BtC;mBAAA;;uBACIC,mBAAA,CA0DM,OA1DNC,UA0DM,G,0BAzDJC,mBAAA,CAA+C;IAA3CH,KAAK,EAAC;EAAY,GAAC,qBAAmB,sBAE1CI,mBAAA,qBAAwB,EACxBD,mBAAA,CAkBM,OAlBNE,UAkBM,GAjBJF,mBAAA,CAOM,OAPNG,UAOM,G,gBANJH,mBAAA,CAKE;IAJAI,IAAI,EAAC,MAAM;IARvB,uBAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IASqBC,KAAA,CAAAC,MAAM,GAAAF,MAAA;IACfG,WAAW,EAAC,oBAAoB;IAChCZ,KAAK,EAAC;iDAFGU,KAAA,CAAAC,MAAM,E,KAKnBR,mBAAA,CAOM,OAPNU,UAOM,G,gBANJV,mBAAA,CAKS;IApBnB,uBAAAK,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAe2BC,KAAA,CAAAI,cAAc,GAAAL,MAAA;IAAET,KAAK,EAAC;gCACrCG,mBAAA,CAAsC;IAA9BY,KAAK,EAAC;EAAE,GAAC,cAAY,uB,kBAC7Bd,mBAAA,CAESe,SAAA,QAnBrBC,WAAA,CAiBqCP,KAAA,CAAAQ,QAAQ,EAAlBC,MAAM;yBAArBlB,mBAAA,CAES;MAF2BmB,GAAG,EAAED,MAAM;MAAGJ,KAAK,EAAEI;wBACpDA,MAAM,wBAlBvBE,UAAA;2EAe2BX,KAAA,CAAAI,cAAc,E,KAOjCX,mBAAA,CAAwE;IAA/DmB,OAAK,EAAAd,MAAA,QAAAA,MAAA,UAAAe,IAAA,KAAEC,QAAA,CAAAC,YAAA,IAAAD,QAAA,CAAAC,YAAA,IAAAF,IAAA,CAAY;IAAEvB,KAAK,EAAC;KAAc,eAAa,E,GAGjEI,mBAAA,uBAA0B,EAC1BD,mBAAA,CAgCQ,SAhCRuB,UAgCQ,G,0BA/BNvB,mBAAA,CASQ,gBARNA,mBAAA,CAOK,aANHA,mBAAA,CAAsB,YAAlB,eAAa,GACjBA,mBAAA,CAAa,YAAT,MAAI,GACRA,mBAAA,CAAa,YAAT,MAAI,GACRA,mBAAA,CAAgB,YAAZ,SAAO,GACXA,mBAAA,CAAe,YAAX,QAAM,GACVA,mBAAA,CAAgB,YAAZ,SAAO,E,wBAGfA,mBAAA,CAoBQ,iB,kBAnBNF,mBAAA,CAkBKe,SAAA,QAxDfC,WAAA,CAsCoCP,KAAA,CAAAiB,oBAAoB,EAAnCC,WAAW;yBAAtB3B,mBAAA,CAkBK;MAlB4CmB,GAAG,EAAEQ,WAAW,CAACC;QAChE1B,mBAAA,CAAuC,YAAA2B,gBAAA,CAAhCF,WAAW,CAACG,YAAY,kBAC/B5B,mBAAA,CAA+B,YAAA2B,gBAAA,CAAxBF,WAAW,CAACI,IAAI,kBACvB7B,mBAAA,CAA+B,YAAA2B,gBAAA,CAAxBF,WAAW,CAACK,IAAI,kBACvB9B,mBAAA,CAAkC,YAAA2B,gBAAA,CAA3BF,WAAW,CAACM,OAAO,kBAC1B/B,mBAAA,CAIK,aAHHA,mBAAA,CAEO;MAFAH,KAAK,EA5C1BmC,eAAA,aA4CwCX,QAAA,CAAAY,cAAc,CAACR,WAAW,CAACT,MAAM;wBACtDS,WAAW,CAACT,MAAM,wB,GAGzBhB,mBAAA,CAOK,aANHA,mBAAA,CAES;MAFDH,KAAK,EAAC,mBAAmB