ify-content: center;\r\n  }\r\n  \r\n  .book-btn {\r\n    background-color: gold;\r\n    color: black;\r\n    padding: 10px 20px;\r\n    border: none;\r\n    border-radius: 5px;\r\n    font-weight: bold;\r\n    text-transform: uppercase;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .book-btn:hover {\r\n    background-color: darkgoldenrod;\r\n  }\r\n  \r\n  .dialog-overlay {\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    right: 0;\r\n    bottom: 0;\r\n    background-color: rgba(0, 0, 0, 0.7);\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    z-index: 1000;\r\n  }\r\n  \r\n  .dialog {\r\n    background-color: black;\r\n    color: white;\r\n    padding: 20px;\r\n    border-radius: 10px;\r\n    width: 400px;\r\n    text-align: center;\r\n  }\r\n  \r\n  .dialog-title {\r\n    font-size: 22px;\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  .dialog-text {\r\n    margin-bottom: 20px;\r\n    font-size: 16px;\r\n  }\r\n  \r\n  .dialog-actions {\r\n    display: flex;\r\n    justify-content: space-around;\r\n  }\r\n  \r\n  .dialog-btn {\r\n    padding: 10px 20px;\r\n    border: none;\r\n    border-radius: 5px;\r\n    font-weight: bold;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .dialog-btn.cancel {\r\n    background-color: red;\r\n  }\r\n  \r\n  .dialog-btn.cancel:hover {\r\n    background-color: darkred;\r\n  }\r\n  \r\n  .dialog-btn.confirm {\r\n    background-color: gold;\r\n    color: black;\r\n  }\r\n  \r\n  .dialog-btn.confirm:hover {\r\n    background-color: darkgoldenrod;\r\n  }\r\n  </style>\r\n  "],"mappings":";;EACSA,KAAK,EAAC;AAAa;;EACjBA,KAAK,EAAC;AAAmB;;EAMrBA,KAAK,EAAC;AAAiB;;EAErBA,KAAK,EAAC;AAAoB;;EACvBA,KAAK,EAAC;AAAO;;EAEhBA,KAAK,EAAC;AAAuB;;EAG7BA,KAAK,EAAC;AAAsB;mBAhB7C;;EAAAC,GAAA;EA0BgCD,KAAK,EAAC;;;EACzBA,KAAK,EAAC;AAAQ;;EAEZA,KAAK,EAAC;AAAa;;EAGnBA,KAAK,EAAC;AAAgB;;uBA/BjCE,mBAAA,CAyCM,OAzCNC,UAyCM,GAxCJC,mBAAA,CAqBM,OArBNC,UAqBM,I,kBApBJH,mBAAA,CAmBMI,SAAA,QAtBdC,WAAA,CAI4BC,KAAA,CAAAC,QAAQ,EAAnBC,OAAO;y