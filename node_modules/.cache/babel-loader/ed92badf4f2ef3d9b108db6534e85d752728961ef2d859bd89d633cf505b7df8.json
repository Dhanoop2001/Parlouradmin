{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'AdminSignUp',\n  data() {\n    return {\n      isPasswordVisible: false,\n      name: '',\n      email: '',\n      password: '',\n      gender: '',\n      showSuccessPopup: false,\n      isSubmitting: false // New property to track button state\n    };\n  },\n  methods: {\n    togglePasswordVisibility() {\n      this.isPasswordVisible = !this.isPasswordVisible;\n    },\n    handleSignUp() {\n      if (this.name && this.email && this.password && this.gender) {\n        this.isSubmitting = true; // Disable the button\n        setTimeout(() => {\n          // Simulating form submission process\n          this.showSuccessPopup = true;\n          this.isSubmitting = false; // Re-enable the button if needed after processing\n        }, 2000); // Simulate a 2-second server response delay\n      } else {\n        alert('Please fill out all fields!');\n      }\n    },\n    closePopup() {\n      this.showSuccessPopup = false;\n      this.$router.push('/log-in'); // Navigate to login page\n    }\n  }\n};","map":{"version":3,"names":["name","data","isPasswordVisible","email","password","gender","showSuccessPopup","isSubmitting","methods","togglePasswordVisibility","handleSignUp","setTimeout","alert","closePopup","$router","push"],"sources":["C:\\Users\\HP-15\\Desktop\\parlouradmin\\parlouradmin\\src\\components\\SignUp.vue"],"sourcesContent":["<template>\r\n    <div class=\"signup-container\">\r\n      <h2 class=\"signup-heading\">Sign Up</h2>\r\n      <form class=\"signup-form\" @submit.prevent=\"handleSignUp\">\r\n        <div class=\"form-group\">\r\n          <label for=\"name\">Name</label>\r\n          <input\r\n            type=\"text\"\r\n            id=\"name\"\r\n            placeholder=\"Enter your name\"\r\n            v-model=\"name\"\r\n            required\r\n          />\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"email\">Email</label>\r\n          <input\r\n            type=\"email\"\r\n            id=\"email\"\r\n            placeholder=\"Enter your email\"\r\n            v-model=\"email\"\r\n            required\r\n          />\r\n        </div>\r\n        <div class=\"form-group password-group\">\r\n          <label for=\"password\">Password</label>\r\n          <div class=\"password-container\">\r\n            <input\r\n              :type=\"isPasswordVisible ? 'text' : 'password'\"\r\n              id=\"password\"\r\n              placeholder=\"Enter your password\"\r\n              v-model=\"password\"\r\n              required\r\n            />\r\n            <button\r\n              type=\"button\"\r\n              class=\"password-toggle\"\r\n              @click=\"togglePasswordVisibility\"\r\n              :aria-label=\"isPasswordVisible ? 'Hide password' : 'Show password'\"\r\n            >\r\n              {{ isPasswordVisible ? 'Hide' : 'Show' }}\r\n            </button>\r\n          </div>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"gender\">Gender</label>\r\n          <select id=\"gender\" v-model=\"gender\" required>\r\n            <option value=\"\" disabled selected>Select your gender</option>\r\n            <option value=\"male\">Men</option>\r\n            <option value=\"female\">Women</option>\r\n          </select>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <button\r\n            type=\"submit\"\r\n            class=\"signup-button\"\r\n            :disabled=\"isSubmitting\"\r\n          >\r\n            {{ isSubmitting ? 'Submitting...' : 'Sign Up' }}\r\n          </button>\r\n        </div>\r\n        <div class=\"form-group already-account\">\r\n          <span>Do you have an account? <router-link to=\"/log-in\">Login</router-link></span>\r\n        </div>\r\n      </form>\r\n  \r\n      <!-- Registration Success Popup -->\r\n      <div v-if=\"showSuccessPopup\" class=\"success-popup\">\r\n        <p>Registration Successful!</p>\r\n        <button @click=\"closePopup\"></button>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    name: 'AdminSignUp',\r\n    data() {\r\n      return {\r\n        isPasswordVisible: false,\r\n        name: '',\r\n        email: '',\r\n        password: '',\r\n        gender: '',\r\n        showSuccessPopup: false,\r\n        isSubmitting: false, // New property to track button state\r\n      };\r\n    },\r\n    methods: {\r\n      togglePasswordVisibility() {\r\n        this.isPasswordVisible = !this.isPasswordVisible;\r\n      },\r\n      handleSignUp() {\r\n        if (this.name && this.email && this.password && this.gender) {\r\n          this.isSubmitting = true; // Disable the button\r\n          setTimeout(() => {\r\n            // Simulating form submission process\r\n            this.showSuccessPopup = true;\r\n            this.isSubmitting = false; // Re-enable the button if needed after processing\r\n          }, 2000); // Simulate a 2-second server response delay\r\n        } else {\r\n          alert('Please fill out all fields!');\r\n        }\r\n      },\r\n      closePopup() {\r\n        this.showSuccessPopup = false;\r\n        this.$router.push('/log-in'); // Navigate to login page\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* Container styles */\r\n  .signup-container {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    justify-content: center;\r\n    height: 100vh;\r\n    background-image: url('@/assets/salon.jpg');\r\n    background-size: cover;\r\n    background-position: center;\r\n    background-repeat: no-repeat;\r\n  }\r\n  \r\n  /* Heading styles */\r\n  .signup-heading {\r\n    font-size: 3rem;\r\n    color: #fff;\r\n    margin-bottom: 1.5rem;\r\n    text-shadow: 2px 2px 10px rgba(0, 0, 0, 0.7);\r\n    font-family: 'Arial', sans-serif;\r\n  }\r\n  \r\n  /* Form styles */\r\n  .signup-form {\r\n    background: linear-gradient(145deg, #333, #444);\r\n    padding: 2.5rem;\r\n    border-radius: 12px;\r\n    box-shadow: 0 10px 20px rgba(0, 0, 0, 0.3);\r\n    width: 90%;\r\n    max-width: 450px;\r\n    transition: transform 0.3s ease, box-shadow 0.3s ease;\r\n  }\r\n  \r\n  .signup-form:hover {\r\n    transform: scale(1.05);\r\n    box-shadow: 0 15px 25px rgba(0, 0, 0, 0.5);\r\n  }\r\n  \r\n  .form-group {\r\n    margin-bottom: 1.5rem;\r\n  }\r\n  \r\n  /* Label and Input styles */\r\n  label {\r\n    display: block;\r\n    margin-bottom: 0.75rem;\r\n    font-weight: 600;\r\n    color: #ddd;\r\n    font-size: 1.1rem;\r\n    font-family: 'Arial', sans-serif;\r\n  }\r\n  \r\n  input, select {\r\n    width: 100%;\r\n    padding: 1rem;\r\n    border: none;\r\n    border-radius: 8px;\r\n    font-size: 1rem;\r\n    background-color: #555;\r\n    color: #fff;\r\n    box-sizing: border-box;\r\n    transition: background-color 0.3s ease, border 0.3s ease;\r\n  }\r\n  \r\n  input:focus, select:focus {\r\n    background-color: #666;\r\n    outline: none;\r\n    border: 2px solid #66bb6a;\r\n  }\r\n  \r\n  /* Password container styles */\r\n  .password-container {\r\n    position: relative;\r\n  }\r\n  \r\n  .password-toggle {\r\n    position: absolute;\r\n    right: 10px;\r\n    top: 50%;\r\n    transform: translateY(-50%);\r\n    background: none;\r\n    border: none;\r\n    color: rgb(24, 204, 24);\r\n    font-size: 0.8rem;\r\n    cursor: pointer;\r\n    text-decoration: underline;\r\n  }\r\n  \r\n  /* Button styles */\r\n  .signup-button {\r\n    width: 100%;\r\n    padding: 1rem;\r\n    background-color: #66bb6a;\r\n    color: #fff;\r\n    border: none;\r\n    border-radius: 8px;\r\n    font-size: 1.1rem;\r\n    font-weight: bold;\r\n    cursor: pointer;\r\n    transition: background-color 0.3s, transform 0.2s ease-in-out;\r\n  }\r\n  \r\n  .signup-button:hover {\r\n    background-color: #4caf50;\r\n    transform: translateY(-2px);\r\n  }\r\n  \r\n  .signup-button:active {\r\n    background-color: #388e3c;\r\n    transform: translateY(1px);\r\n  }\r\n  \r\n  .signup-button:disabled {\r\n    background-color: #9e9e9e;\r\n    cursor: not-allowed;\r\n  }\r\n  \r\n  /* Already have account text styles */\r\n  .already-account {\r\n    text-align: center;\r\n    font-size: 1rem;\r\n    color: #ccc;\r\n  }\r\n  \r\n  .already-account a {\r\n    color: #66bb6a;\r\n    text-decoration: none;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  .already-account a:hover {\r\n    text-decoration: underline;\r\n  }\r\n  \r\n  /* Popup styles */\r\n  .success-popup {\r\n    position: fixed;\r\n    top: 50%;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%);\r\n    background: #fff;\r\n    padding: 2rem;\r\n    border-radius: 8px;\r\n    box-shadow: 0 10px 20px rgba(0, 0, 0, 0.3);\r\n    text-align: center;\r\n    z-index: 1000;\r\n  }\r\n  \r\n  .success-popup p {\r\n    font-size: 1.2rem;\r\n    color: #333;\r\n    margin-bottom: 1rem;\r\n  }\r\n  \r\n  .success-popup button {\r\n    padding: 0.5rem 1.5rem;\r\n    background-color: #66bb6a;\r\n    color: #fff;\r\n    border: none;\r\n    border-radius: 8px;\r\n    font-size: 1rem;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .success-popup button:hover {\r\n    background-color: #4caf50;\r\n  }\r\n  </style>\r\n  "],"mappings":";AA2EE,eAAe;EACbA,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,iBAAiB,EAAE,KAAK;MACxBF,IAAI,EAAE,EAAE;MACRG,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE,EAAE;MACVC,gBAAgB,EAAE,KAAK;MACvBC,YAAY,EAAE,KAAK,CAAE;IACvB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,wBAAwBA,CAAA,EAAG;MACzB,IAAI,CAACP,iBAAgB,GAAI,CAAC,IAAI,CAACA,iBAAiB;IAClD,CAAC;IACDQ,YAAYA,CAAA,EAAG;MACb,IAAI,IAAI,CAACV,IAAG,IAAK,IAAI,CAACG,KAAI,IAAK,IAAI,CAACC,QAAO,IAAK,IAAI,CAACC,MAAM,EAAE;QAC3D,IAAI,CAACE,YAAW,GAAI,IAAI,EAAE;QAC1BI,UAAU,CAAC,MAAM;UACf;UACA,IAAI,CAACL,gBAAe,GAAI,IAAI;UAC5B,IAAI,CAACC,YAAW,GAAI,KAAK,EAAE;QAC7B,CAAC,EAAE,IAAI,CAAC,EAAE;MACZ,OAAO;QACLK,KAAK,CAAC,6BAA6B,CAAC;MACtC;IACF,CAAC;IACDC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACP,gBAAe,GAAI,KAAK;MAC7B,IAAI,CAACQ,OAAO,CAACC,IAAI,CAAC,SAAS,CAAC,EAAE;IAChC;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}