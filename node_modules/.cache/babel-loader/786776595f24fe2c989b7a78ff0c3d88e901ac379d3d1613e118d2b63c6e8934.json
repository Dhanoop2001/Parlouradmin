{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nexport default {\n  data() {\n    return {\n      staff: {\n        name: '',\n        role: '',\n        contact: ''\n      },\n      staffList: [{\n        name: 'Anna Belle',\n        role: 'stylists',\n        contact: 'anna@example.com'\n      }, {\n        name: 'John Spa',\n        role: 'Therapists',\n        contact: 'john@example.com'\n      }, {\n        name: 'Sara Lee',\n        role: 'Receptionists',\n        contact: 'sara@example.com'\n      }, {\n        name: 'James Cut',\n        role: 'Hair Cutting',\n        contact: 'james@example.com'\n      } // Hair Cutting Staff\n      ],\n      showModal: false,\n      isEditing: false,\n      editIndex: null,\n      filteredStaff: []\n    };\n  },\n  methods: {\n    handleSubmit() {\n      if (this.isEditing) {\n        this.staffList[this.editIndex] = {\n          ...this.staff\n        };\n      } else {\n        if (!this.staff.role) {\n          alert(\"Role cannot be empty!\");\n          return;\n        }\n        this.staffList.push({\n          ...this.staff\n        });\n      }\n      this.resetForm();\n    },\n    editStaff(index) {\n      this.staff = {\n        ...this.staffList[index]\n      };\n      this.isEditing = true;\n      this.editIndex = index;\n      this.showModal = true;\n    },\n    removeStaff(index) {\n      this.staffList.splice(index, 1);\n    },\n    resetForm() {\n      this.staff = {\n        name: '',\n        role: '',\n        contact: ''\n      };\n      this.isEditing = false;\n      this.editIndex = null;\n      this.showModal = false;\n    },\n    filterCategory(category) {\n      if (category === 'all') {\n        this.filteredStaff = this.staffList;\n      } else {\n        this.filteredStaff = this.staffList.filter(staff => staff.role.toLowerCase() === category.toLowerCase() // Ensure exact match\n        );\n      }\n    }\n  },\n  mounted() {\n    this.filteredStaff = this.staffList;\n  }\n};","map":{"version":3,"names":["data","staff","name","role","contact","staffList","showModal","isEditing","editIndex","filteredStaff","methods","handleSubmit","alert","push","resetForm","editStaff","index","removeStaff","splice","filterCategory","category","filter","toLowerCase","mounted"],"sources":["C:\\Users\\HP-15\\Desktop\\parlouradmin\\parlouradmin\\src\\components\\ManageStaff.vue"],"sourcesContent":["<template>\r\n    <div class=\"admin-dashboard\">\r\n      <!-- Sidebar -->\r\n      <aside class=\"sidebar\">\r\n        <div class=\"logo\">\r\n          <h1>Salon Info</h1>\r\n        </div>\r\n        <nav class=\"navigation\">\r\n          <ul>\r\n            <li @click=\"filterCategory('all')\">All Staff</li>\r\n            <li @click=\"filterCategory('stylists')\">Stylists</li>\r\n            <li @click=\"filterCategory('therapists')\">Therapists</li>\r\n            <li @click=\"filterCategory('receptionists')\">Receptionists</li>\r\n            <li @click=\"filterCategory('hair cutting')\">Hair Cutting</li> <!-- Hair Cutting Category -->\r\n          </ul>\r\n        </nav>\r\n      </aside>\r\n  \r\n      <!-- Main Content -->\r\n      <main class=\"main-content\">\r\n        <header class=\"header\">\r\n          <h1>Manage Salon Info Staff</h1>\r\n          <button @click=\"showModal = true\" class=\"add-staff-btn\">+ Add Staff</button>\r\n        </header>\r\n  \r\n        <!-- Staff Table -->\r\n        <div class=\"staff-table\">\r\n          <table>\r\n            <thead>\r\n              <tr>\r\n                <th>Name</th>\r\n                <th>Role</th>\r\n                <th>Contact</th>\r\n                <th>Actions</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"(staff, index) in filteredStaff\" :key=\"index\">\r\n                <td>{{ staff.name }}</td>\r\n                <td>{{ staff.role }}</td>\r\n                <td>{{ staff.contact }}</td>\r\n                <td class=\"actions\">\r\n                  <button @click=\"editStaff(index)\">✏️</button>\r\n                  <button @click=\"removeStaff(index)\">❌</button>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </main>\r\n  \r\n      <!-- Add/Edit Staff Modal -->\r\n      <div v-if=\"showModal\" class=\"modal-overlay\">\r\n        <div class=\"modal\">\r\n          <h2>{{ isEditing ? 'Edit Staff' : 'Add Staff' }}</h2>\r\n          <form @submit.prevent=\"handleSubmit\">\r\n            <input v-model=\"staff.name\" type=\"text\" placeholder=\"Full Name\" required />\r\n            <!-- Role Dropdown -->\r\n            <select v-model=\"staff.role\" required>\r\n              <option value=\"\" disabled selected>Select Role</option>\r\n              <option value=\"Stylists\">Stylists</option>\r\n              <option value=\"Therapists\">Therapists</option>\r\n              <option value=\"Receptionists\">Receptionists</option>\r\n              <option value=\"Hair Cutting\">Hair Cutting</option>\r\n            </select>\r\n            <input v-model=\"staff.contact\" type=\"email\" placeholder=\"Email\" required />\r\n            <button type=\"submit\" class=\"save-btn\">{{ isEditing ? 'Save Changes' : 'Add Staff' }}</button>\r\n          </form>\r\n          <button class=\"close-btn\" @click=\"resetForm\">✖</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        staff: { name: '', role: '', contact: '' },\r\n        staffList: [\r\n          { name: 'Anna Belle', role: 'stylists', contact: 'anna@example.com' },\r\n          { name: 'John Spa', role: 'Therapists', contact: 'john@example.com' },\r\n          { name: 'Sara Lee', role: 'Receptionists', contact: 'sara@example.com' },\r\n          { name: 'James Cut', role: 'Hair Cutting', contact: 'james@example.com' }, // Hair Cutting Staff\r\n        ],\r\n        showModal: false,\r\n        isEditing: false,\r\n        editIndex: null,\r\n        filteredStaff: [],\r\n      };\r\n    },\r\n    methods: {\r\n      handleSubmit() {\r\n        if (this.isEditing) {\r\n          this.staffList[this.editIndex] = { ...this.staff };\r\n        } else {\r\n          if (!this.staff.role) {\r\n            alert(\"Role cannot be empty!\");\r\n            return;\r\n          }\r\n          this.staffList.push({ ...this.staff });\r\n        }\r\n        this.resetForm();\r\n      },\r\n      editStaff(index) {\r\n        this.staff = { ...this.staffList[index] };\r\n        this.isEditing = true;\r\n        this.editIndex = index;\r\n        this.showModal = true;\r\n      },\r\n      removeStaff(index) {\r\n        this.staffList.splice(index, 1);\r\n      },\r\n      resetForm() {\r\n        this.staff = { name: '', role: '', contact: '' };\r\n        this.isEditing = false;\r\n        this.editIndex = null;\r\n        this.showModal = false;\r\n      },\r\n      filterCategory(category) {\r\n        if (category === 'all') {\r\n          this.filteredStaff = this.staffList;\r\n        } else {\r\n          this.filteredStaff = this.staffList.filter((staff) =>\r\n            staff.role.toLowerCase() === category.toLowerCase()  // Ensure exact match\r\n          );\r\n        }\r\n      },\r\n    },\r\n    mounted() {\r\n      this.filteredStaff = this.staffList;\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* General Styling */\r\n  .admin-dashboard {\r\n    display: flex;\r\n    min-height: 100vh;\r\n    background: white;\r\n    color: black;\r\n    font-family: 'Arial', sans-serif;\r\n  }\r\n  \r\n  /* Sidebar */\r\n  .sidebar {\r\n    width: 25%;\r\n    background: black;\r\n    color: gold;\r\n    padding: 20px;\r\n    text-align: center;\r\n  }\r\n  \r\n  .sidebar .logo {\r\n    font-size: 1.8rem;\r\n    font-weight: bold;\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  .sidebar ul {\r\n    list-style: none;\r\n    padding: 0;\r\n  }\r\n  \r\n  .sidebar li {\r\n    padding: 12px 15px;\r\n    margin: 10px 0;\r\n    cursor: pointer;\r\n    text-transform: capitalize;\r\n    background: gold;\r\n    color: black;\r\n    border-radius: 5px;\r\n  }\r\n  \r\n  .sidebar li:hover {\r\n    background: black;\r\n    color: gold;\r\n    border: 1px solid gold;\r\n  }\r\n  \r\n  /* Main Content */\r\n  .main-content {\r\n    flex: 1;\r\n    padding: 20px;\r\n  }\r\n  \r\n  .header {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n  }\r\n  \r\n  .add-staff-btn {\r\n    background-color: gold;\r\n    color: black;\r\n    padding: 10px 20px;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n    font-size: 1rem;\r\n  }\r\n  \r\n  .add-staff-btn:hover {\r\n    background-color: black;\r\n    color: gold;\r\n  }\r\n  \r\n  /* Staff Table */\r\n  .staff-table table {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n    margin-top: 20px;\r\n  }\r\n  \r\n  .staff-table th,\r\n  .staff-table td {\r\n    padding: 10px;\r\n    border: 1px solid #e0e0e0;\r\n    text-align: left;\r\n  }\r\n  \r\n  .staff-table th {\r\n    background-color: gold;\r\n    color: black;\r\n  }\r\n  \r\n  .actions button {\r\n    background: none;\r\n    border: none;\r\n    cursor: pointer;\r\n    font-size: 1.2rem;\r\n  }\r\n  \r\n  .actions button:hover {\r\n    color: gold;\r\n  }\r\n  \r\n  /* Modal */\r\n  .modal-overlay {\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    background: rgba(0, 0, 0, 0.5);\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n  \r\n  .modal {\r\n    background: white;\r\n    padding: 40px;\r\n    border-radius: 10px;\r\n    width: 400px;\r\n    position: relative;\r\n  }\r\n  \r\n  .modal h2 {\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  .modal form {\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap: 10px;\r\n  }\r\n  \r\n  .modal form input,\r\n  .modal form select {\r\n    height: 40px;\r\n    padding: 5px;\r\n    font-size: 1rem;\r\n    border: 1px solid #ccc;\r\n    border-radius: 5px;\r\n  }\r\n  \r\n  .save-btn {\r\n    height: 40px;\r\n    cursor: pointer;\r\n    background: gold;\r\n    color: black;\r\n    border: none;\r\n    border-radius: 5px;\r\n  }\r\n  \r\n  .save-btn:hover {\r\n    background: black;\r\n    color: gold;\r\n  }\r\n  \r\n  .close-btn {\r\n    position: absolute;\r\n    top: 10px;\r\n    right: 10px;\r\n    background: none;\r\n    border: none;\r\n    font-size: 1.5rem;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .close-btn:hover {\r\n    color: gold;\r\n  }\r\n  </style>\r\n  "],"mappings":";;;AA2EE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;QAAEC,IAAI,EAAE,EAAE;QAAEC,IAAI,EAAE,EAAE;QAAEC,OAAO,EAAE;MAAG,CAAC;MAC1CC,SAAS,EAAE,CACT;QAAEH,IAAI,EAAE,YAAY;QAAEC,IAAI,EAAE,UAAU;QAAEC,OAAO,EAAE;MAAmB,CAAC,EACrE;QAAEF,IAAI,EAAE,UAAU;QAAEC,IAAI,EAAE,YAAY;QAAEC,OAAO,EAAE;MAAmB,CAAC,EACrE;QAAEF,IAAI,EAAE,UAAU;QAAEC,IAAI,EAAE,eAAe;QAAEC,OAAO,EAAE;MAAmB,CAAC,EACxE;QAAEF,IAAI,EAAE,WAAW;QAAEC,IAAI,EAAE,cAAc;QAAEC,OAAO,EAAE;MAAoB,CAAC,CAAE;MAAA,CAC5E;MACDE,SAAS,EAAE,KAAK;MAChBC,SAAS,EAAE,KAAK;MAChBC,SAAS,EAAE,IAAI;MACfC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACb,IAAI,IAAI,CAACJ,SAAS,EAAE;QAClB,IAAI,CAACF,SAAS,CAAC,IAAI,CAACG,SAAS,IAAI;UAAE,GAAG,IAAI,CAACP;QAAM,CAAC;MACpD,OAAO;QACL,IAAI,CAAC,IAAI,CAACA,KAAK,CAACE,IAAI,EAAE;UACpBS,KAAK,CAAC,uBAAuB,CAAC;UAC9B;QACF;QACA,IAAI,CAACP,SAAS,CAACQ,IAAI,CAAC;UAAE,GAAG,IAAI,CAACZ;QAAM,CAAC,CAAC;MACxC;MACA,IAAI,CAACa,SAAS,CAAC,CAAC;IAClB,CAAC;IACDC,SAASA,CAACC,KAAK,EAAE;MACf,IAAI,CAACf,KAAI,GAAI;QAAE,GAAG,IAAI,CAACI,SAAS,CAACW,KAAK;MAAE,CAAC;MACzC,IAAI,CAACT,SAAQ,GAAI,IAAI;MACrB,IAAI,CAACC,SAAQ,GAAIQ,KAAK;MACtB,IAAI,CAACV,SAAQ,GAAI,IAAI;IACvB,CAAC;IACDW,WA