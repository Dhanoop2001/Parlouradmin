{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nexport default {\n  data() {\n    return {\n      appointments: [],\n      search: '',\n      selectedStatus: '',\n      statuses: ['Confirmed', 'Pending', 'Cancelled', 'Completed'],\n      isEditModalVisible: false,\n      editedAppointment: null\n    };\n  },\n  computed: {\n    filteredAppointments() {\n      return this.appointments.filter(appointment => {\n        const matchesSearch = !this.search || appointment.customerName.toLowerCase().includes(this.search.toLowerCase());\n        const matchesStatus = !this.selectedStatus || appointment.status === this.selectedStatus;\n        return matchesSearch && matchesStatus;\n      });\n    }\n  },\n  methods: {\n    fetchAppointments() {\n      this.appointments = [{\n        id: 1,\n        customerName: 'Alice Johnson',\n        date: '2024-12-01',\n        time: '10:00',\n        timePeriod: 'AM',\n        service: 'Haircut',\n        status: 'Confirmed'\n      }, {\n        id: 2,\n        customerName: 'Bob Smith',\n        date: '2024-12-02',\n        time: '2:00',\n        timePeriod: 'PM',\n        service: 'Facial',\n        status: 'Pending'\n      }, {\n        id: 3,\n        customerName: 'Sarah Lee',\n        date: '2024-12-03',\n        time: '4:00',\n        timePeriod: 'PM',\n        service: 'Massage',\n        status: 'Completed'\n      }];\n    },\n    getStatusClass(status) {\n      switch (status) {\n        case 'Confirmed':\n          return 'status-confirmed';\n        case 'Pending':\n          return 'status-pending';\n        case 'Cancelled':\n          return 'status-cancelled';\n        case 'Completed':\n          return 'status-completed';\n        default:\n          return '';\n      }\n    },\n    editAppointment(appointment) {\n      this.editedAppointment = {\n        ...appointment\n      }; // Clone the selected appointment\n      this.isEditModalVisible = true;\n    },\n    saveAppointment() {\n      const index = this.appointments.findIndex(appointment => appointment.id === this.editedAppointment.id);\n      if (index !== -1) {\n        this.appointments.splice(index, 1, this.editedAppointment);\n      }\n      this.closeEditModal();\n    },\n    deleteAppointment(appointmentId) {\n      this.appointments = this.appointments.filter(appointment => appointment.id !== appointmentId);\n      console.log('Deleted appointment', appointmentId);\n    },\n    closeEditModal() {\n      this.isEditModalVisible = false;\n      this.editedAppointment = null;\n    }\n  },\n  mounted() {\n    this.fetchAppointments();\n  }\n};","map":{"version":3,"names":["data","appointments","search","selectedStatus","statuses","isEditModalVisible","editedAppointment","computed","filteredAppointments","filter","appointment","matchesSearch","customerName","toLowerCase","includes","matchesStatus","status","methods","fetchAppointments","id","date","time","timePeriod","service","getStatusClass","editAppointment","saveAppointment","index","findIndex","splice","closeEditModal","deleteAppointment","appointmentId","console","log","mounted"],"sources":["C:\\Users\\HP-15\\Desktop\\parlouradmin\\parlouradmin\\src\\components\\ManageStaff.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h1 class=\"page-title\">Manage Appointments</h1>\r\n\r\n    <!-- Filters Section -->\r\n    <div class=\"filters mb-4\">\r\n      <div class=\"filter-group\">\r\n        <input\r\n          type=\"text\"\r\n          v-model=\"search\"\r\n          placeholder=\"Search by Customer\"\r\n          class=\"filter-input\"\r\n        />\r\n      </div>\r\n      <div class=\"filter-group\">\r\n        <select v-model=\"selectedStatus\" class=\"filter-select\">\r\n          <option value=\"\">All Statuses</option>\r\n          <option v-for=\"status in statuses\" :key=\"status\" :value=\"status\">\r\n            {{ status }}\r\n          </option>\r\n        </select>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Appointment Table -->\r\n    <table class=\"appointment-table\">\r\n      <thead>\r\n        <tr>\r\n          <th>Customer Name</th>\r\n          <th>Date</th>\r\n          <th>Time</th>\r\n          <th>Service</th>\r\n          <th>Status</th>\r\n          <th>Actions</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"appointment in filteredAppointments\" :key=\"appointment.id\">\r\n          <td>{{ appointment.customerName }}</td>\r\n          <td>{{ appointment.date }}</td>\r\n          <td>{{ appointment.time }} {{ appointment.timePeriod }}</td>\r\n          <td>{{ appointment.service }}</td>\r\n          <td>\r\n            <span :class=\"'status ' + getStatusClass(appointment.status)\">\r\n              {{ appointment.status }}\r\n            </span>\r\n          </td>\r\n          <td>\r\n            <button class=\"btn-icon btn-edit\" @click=\"editAppointment(appointment)\">\r\n              ‚úèÔ∏è\r\n            </button>\r\n            <button class=\"btn-icon btn-delete\" @click=\"deleteAppointment(appointment.id)\">\r\n              üóëÔ∏è\r\n            </button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- Edit Appointment Modal -->\r\n    <div v-if=\"isEditModalVisible\" class=\"modal\">\r\n      <div class=\"modal-content\">\r\n        <h2>Edit Appointment</h2>\r\n        <form @submit.prevent=\"saveAppointment\">\r\n          <div class=\"form-group\">\r\n            <label for=\"customerName\">Customer Name:</label>\r\n            <input\r\n              id=\"customerName\"\r\n              type=\"text\"\r\n              v-model=\"editedAppointment.customerName\"\r\n              required\r\n              class=\"form-input\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"date\">Date:</label>\r\n            <input\r\n              id=\"date\"\r\n              type=\"date\"\r\n              v-model=\"editedAppointment.date\"\r\n              required\r\n              class=\"form-input\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"time\">Time:</label>\r\n            <div class=\"time-container\">\r\n              <input\r\n                id=\"time\"\r\n                type=\"time\"\r\n                v-model=\"editedAppointment.time\"\r\n                required\r\n                class=\"form-input\"\r\n                step=\"900\"\r\n              />\r\n              <select\r\n                v-model=\"editedAppointment.timePeriod\"\r\n                required\r\n                class=\"form-input time-period\"\r\n              >\r\n                <option value=\"AM\">AM</option>\r\n                <option value=\"PM\">PM</option>\r\n              </select>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"service\">Service:</label>\r\n            <input\r\n              id=\"service\"\r\n              type=\"text\"\r\n              v-model=\"editedAppointment.service\"\r\n              required\r\n              class=\"form-input\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"status\">Status:</label>\r\n            <select\r\n              id=\"status\"\r\n              v-model=\"editedAppointment.status\"\r\n              required\r\n              class=\"form-input\"\r\n            >\r\n              <option value=\"Confirmed\">Confirmed</option>\r\n              <option value=\"Pending\">Pending</option>\r\n              <option value=\"Cancelled\">Cancelled</option>\r\n              <option value=\"Completed\">Completed</option>\r\n            </select>\r\n          </div>\r\n\r\n          <div class=\"modal-buttons\">\r\n            <button type=\"submit\" class=\"btn-submit\">Save</button>\r\n            <button type=\"button\" class=\"btn-cancel\" @click=\"closeEditModal\">Cancel</button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      appointments: [],\r\n      search: '',\r\n      selectedStatus: '',\r\n      statuses: ['Confirmed', 'Pending', 'Cancelled', 'Completed'],\r\n      isEditModalVisible: false,\r\n      editedAppointment: null,\r\n    };\r\n  },\r\n  computed: {\r\n    filteredAppointments() {\r\n      return this.appointments.filter((appointment) => {\r\n        const matchesSearch =\r\n          !this.search ||\r\n          appointment.customerName\r\n            .toLowerCase()\r\n            .includes(this.search.toLowerCase());\r\n        const matchesStatus =\r\n          !this.selectedStatus || appointment.status === this.selectedStatus;\r\n        return matchesSearch && matchesStatus;\r\n      });\r\n    },\r\n  },\r\n  methods: {\r\n    fetchAppointments() {\r\n      this.appointments = [\r\n        {\r\n          id: 1,\r\n          customerName: 'Alice Johnson',\r\n          date: '2024-12-01',\r\n          time: '10:00',\r\n          timePeriod: 'AM',\r\n          service: 'Haircut',\r\n          status: 'Confirmed',\r\n        },\r\n        {\r\n          id: 2,\r\n          customerName: 'Bob Smith',\r\n          date: '2024-12-02',\r\n          time: '2:00',\r\n          timePeriod: 'PM',\r\n          service: 'Facial',\r\n          status: 'Pending',\r\n        },\r\n        {\r\n          id: 3,\r\n          customerName: 'Sarah Lee',\r\n          date: '2024-12-03',\r\n          time: '4:00',\r\n          timePeriod: 'PM',\r\n          service: 'Massage',\r\n          status: 'Completed',\r\n        },\r\n      ];\r\n    },\r\n    getStatusClass(status) {\r\n      switch (status) {\r\n        case 'Confirmed':\r\n          return 'status-confirmed';\r\n        case 'Pending':\r\n          return 'status-pending';\r\n        case 'Cancelled':\r\n          return 'status-cancelled';\r\n        case 'Completed':\r\n          return 'status-completed';\r\n        default:\r\n          return '';\r\n      }\r\n    },\r\n    editAppointment(appointment) {\r\n      this.editedAppointment = { ...appointment }; // Clone the selected appointment\r\n      this.isEditModalVisible = true;\r\n    },\r\n    saveAppointment() {\r\n      const index = this.appointments.findIndex(\r\n        (appointment) => appointment.id === this.editedAppointment.id\r\n      );\r\n      if (index !== -1) {\r\n        this.appointments.splice(index, 1, this.editedAppointment);\r\n      }\r\n      this.closeEditModal();\r\n    },\r\n    deleteAppointment(appointmentId) {\r\n      this.appointments = this.appointments.filter(\r\n        (appointment) => appointment.id !== appointmentId\r\n      );\r\n      console.log('Deleted appointment', appointmentId);\r\n    },\r\n    closeEditModal() {\r\n      this.isEditModalVisible = false;\r\n      this.editedAppointment = null;\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchAppointments();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Background Styling */\r\n.container {\r\n  background: url('@/assets/salon.jpg') no-repeat center center fixed;\r\n  background-size: cover;\r\n  color: #fff;\r\n  min-height: 100vh;\r\n  padding: 20px;\r\n  box-sizing: border-box;\r\n  position: relative;\r\n}\r\n\r\n.container::before {\r\n  content: '';\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  background-color: rgba(0, 0, 0, 0.712); /* Light black tint */\r\n  z-index: -1;\r\n}\r\n\r\n/* Page Title Styling */\r\n.page-title {\r\n  text-align: center;\r\n  font-size: 28px;\r\n  font-weight: bold;\r\n  color: #fff;\r\n  margin-bottom: 30px;\r\n  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.6);\r\n}\r\n\r\n/* Filters Section */\r\n.filters {\r\n  display: flex;\r\n  gap: 10px;\r\n  flex-wrap: wrap;\r\n  background-color: rgba(255, 255, 255, 0.8);\r\n  padding: 15px;\r\n  border-radius: 8px;\r\n}\r\n\r\n.filter-input,\r\n.filter-select {\r\n  width: 100%;\r\n  padding: 8px;\r\n  font-size: 16px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px;\r\n}\r\n\r\n/* Appointment Table */\r\n.appointment-table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  margin-top: 20px;\r\n  background-color: #fff; /* White table background */\r\n  border-radius: 10px;\r\n  overflow: hidden;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.appointment-table th,\r\n.appointment-table td {\r\n  padding: 12px;\r\n  text-align: left;\r\n  border: 1px solid #ddd;\r\n  color: #000; /* Black text color */\r\n}\r\n\r\n.appointment-table th {\r\n  background-color: #000; /* Black background for headers */\r\n  color: #ffd700; /* Gold text for headers */\r\n  font-weight: bold;\r\n}\r\n\r\n.appointment-table tr:nth-child(even) {\r\n  background-color: #f4f4f4; /* Light grey for even rows */\r\n}\r\n\r\n.appointment-table tr:hover {\r\n  background-color: #ddd; /* Light grey on hover */\r\n}\r\n\r\n/* Status Styles */\r\n.status {\r\n  padding: 5px 10px;\r\n  border-radius: 5px;\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n  display: inline-block;\r\n}\r\n\r\n.status-confirmed {\r\n  background-color: #28a745;\r\n  color: white;\r\n}\r\n\r\n.status-pending {\r\n  background-color: #ffc107;\r\n  color: white;\r\n}\r\n\r\n.status-cancelled {\r\n  background-color: #dc3545;\r\n  color: white;\r\n}\r\n\r\n.status-completed {\r\n  background-color: #17a2b8;\r\n  color: white;\r\n}\r\n\r\n/* Buttons */\r\n.btn-icon {\r\n  background: none;\r\n  border: none;\r\n  cursor: pointer;\r\n  font-size: 18px;\r\n  margin: 0 5px;\r\n}\r\n\r\n.btn-icon:hover {\r\n  opacity: 0.8;\r\n}\r\n\r\n.btn-edit {\r\n  color: #ffd700; /* Gold for edit button */\r\n}\r\n\r\n.btn-delete {\r\n  color: #dc3545; /* Red for delete button */\r\n}\r\n\r\n/* Modal Styles */\r\n.modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  background: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.modal-content {\r\n  background: #fff; /* White background for modal */\r\n  padding: 30px;\r\n  border-radius: 10px;\r\n  max-width: 500px;\r\n  width: 100%;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 15px;\r\n}\r\n\r\n.form-group label {\r\n  display: block;\r\n  font-size: 16px;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.form-input {\r\n  width: 100%;\r\n  padding: 10px;\r\n  font-size: 16px;\r\n  border-radius: 5px;\r\n  border: 1px solid #ccc;\r\n}\r\n\r\n.modal-buttons {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  margin-top: 20px;\r\n}\r\n\r\n.btn-submit,\r\n.btn-cancel {\r\n  padding: 10px 20px;\r\n  font-size: 16px;\r\n  border-radius: 5px;\r\n}\r\n\r\n.btn-submit {\r\n  background-color: gold; /* Green for save button */\r\n  color: black;\r\n  border: none;\r\n}\r\n\r\n.btn-submit:hover {\r\n  background-color: #218838;\r\n}\r\n\r\n.btn-cancel {\r\n  background-color: #dc3545; /* Red for cancel button */\r\n  color: white;\r\n  border: none;\r\n}\r\n\r\n.btn-cancel:hover {\r\n  background-color: #c82333;\r\n}\r\n\r\n/* Time Container */\r\n.time-container {\r\n  display: flex;\r\n  gap: 5px;\r\n}\r\n\r\n.time-period {\r\n  width: 70px;\r\n  font-size: 16px;\r\n}\r\n</style>\r\n"],"mappings":";;AAiJA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,EAAE;MAChBC,MAAM,EAAE,EAAE;MACVC,cAAc,EAAE,EAAE;MAClBC,QAAQ,EAAE,CAAC,WAAW,EAAE,SAAS,EAAE,WAAW,EAAE,WAAW,CAAC;MAC5DC,kBAAkB,EAAE,KAAK;MACzBC,iBAAiB,EAAE;IACrB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,oBAAoBA,CAAA,EAAG;MACrB,OAAO,IAAI,CAACP,YAAY,CAACQ,MAAM,CAAEC,WAAW,IAAK;QAC/C,MAAMC,aAAY,GAChB,CAAC,IAAI,CAACT,MAAK,IACXQ,WAAW,CAACE,YAAW,CACpBC,WAAW,CAAC,EACZC,QAAQ,CAAC,IAAI,CAACZ,MAAM,CAACW,WAAW,CAAC,CAAC,CAAC;QACxC,MAAME,aAAY,GAChB,CAAC,IAAI,CAACZ,cAAa,IAAKO,WAAW,CAACM,MAAK,KAAM,IAAI,CAACb