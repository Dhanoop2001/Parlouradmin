{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      services: [{\n        name: \"Haircut\",\n        description: \"Experience a fresh, stylish haircut.\"\n      }, {\n        name: \"Facial\",\n        description: \"Revitalize your skin with a relaxing facial.\"\n      }, {\n        name: \"Massage\",\n        description: \"Relax your body with a soothing massage.\"\n      }\n      // Add more services as needed\n      ],\n      dialogVisible: false,\n      selectedService: null\n    };\n  },\n  methods: {\n    bookService(serviceName) {\n      // Set the selected service and show the confirmation dialog\n      this.selectedService = serviceName;\n      this.dialogVisible = true;\n    },\n    handleBooking() {\n      // Handle the booking process after confirmation\n      console.log(`Service '${this.selectedService}' booked successfully!`);\n\n      // Close the dialog\n      this.dialogVisible = false;\n\n      // For example, navigate to a booking confirmation page (if you have Vue Router)\n      this.$router.push({\n        name: 'BookingConfirmation',\n        params: {\n          service: this.selectedService\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","services","name","description","dialogVisible","selectedService","methods","bookService","serviceName","handleBooking","console","log","$router","push","params","service"],"sources":["C:\\Users\\HP-15\\Desktop\\parlouradmin\\parlouradmin\\src\\components\\ClientPage.vue"],"sourcesContent":["<template>\r\n    <v-container class=\"client-page\" fluid>\r\n      <v-row class=\"d-flex justify-center\">\r\n        <v-col\r\n          v-for=\"service in services\"\r\n          :key=\"service.name\"\r\n          cols=\"12\"\r\n          md=\"4\"\r\n        >\r\n          <v-card class=\"service-card\" :style=\"{ backgroundImage: 'url(/salon.jpg)', backgroundSize: 'cover' }\">\r\n            <v-card-title class=\"service-card-title\">\r\n              <span class=\"title\">{{ service.name }}</span>\r\n            </v-card-title>\r\n            <v-card-subtitle class=\"service-card-subtitle\">\r\n              {{ service.description }}\r\n            </v-card-subtitle>\r\n            <v-card-actions class=\"service-card-actions\">\r\n              <v-btn color=\"gold\" @click=\"bookService(service.name)\">\r\n                Book Now\r\n              </v-btn>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  \r\n    <!-- Booking Confirmation Dialog -->\r\n    <v-dialog v-model=\"dialogVisible\" max-width=\"500px\">\r\n      <v-card>\r\n        <v-card-title class=\"headline\">Confirm Booking</v-card-title>\r\n        <v-card-text>\r\n          Are you sure you want to book the <strong>{{ selectedService }}</strong> service?\r\n        </v-card-text>\r\n        <v-card-actions>\r\n          <v-btn text @click=\"dialogVisible = false\">Cancel</v-btn>\r\n          <v-btn color=\"gold\" @click=\"handleBooking\">Book Now</v-btn>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        services: [\r\n          {\r\n            name: \"Haircut\",\r\n            description: \"Experience a fresh, stylish haircut.\",\r\n          },\r\n          {\r\n            name: \"Facial\",\r\n            description: \"Revitalize your skin with a relaxing facial.\",\r\n          },\r\n          {\r\n            name: \"Massage\",\r\n            description: \"Relax your body with a soothing massage.\",\r\n          },\r\n          // Add more services as needed\r\n        ],\r\n        dialogVisible: false,\r\n        selectedService: null,\r\n      };\r\n    },\r\n    methods: {\r\n      bookService(serviceName) {\r\n        // Set the selected service and show the confirmation dialog\r\n        this.selectedService = serviceName;\r\n        this.dialogVisible = true;\r\n      },\r\n      handleBooking() {\r\n        // Handle the booking process after confirmation\r\n        console.log(`Service '${this.selectedService}' booked successfully!`);\r\n        \r\n        // Close the dialog\r\n        this.dialogVisible = false;\r\n  \r\n        // For example, navigate to a booking confirmation page (if you have Vue Router)\r\n        this.$router.push({ name: 'BookingConfirmation', params: { service: this.selectedService } });\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .client-page {\r\n    background-color: black;\r\n    color: white;\r\n    min-height: 100vh;\r\n    padding: 0;\r\n  }\r\n  \r\n  .service-card {\r\n    border-radius: 10px;\r\n    color: white;\r\n    overflow: hidden;\r\n    box-shadow: 0 8px 16px rgba(0, 0, 0, 0.5);\r\n    transition: transform 0.3s ease-in-out;\r\n  }\r\n  \r\n  .service-card:hover {\r\n    transform: scale(1.05);\r\n  }\r\n  \r\n  .service-card-title {\r\n    background-color: rgba(0, 0, 0, 0.6);\r\n    padding: 10px;\r\n    font-weight: bold;\r\n    text-align: center;\r\n  }\r\n  \r\n  .service-card-subtitle {\r\n    background-color: rgba(0, 0, 0, 0.5);\r\n    padding: 10px;\r\n    font-size: 16px;\r\n  }\r\n  \r\n  .service-card-actions {\r\n    background-color: rgba(0, 0, 0, 0.6);\r\n    padding: 10px;\r\n    display: flex;\r\n    justify-content: center;\r\n  }\r\n  \r\n  .v-btn {\r\n    background-color: gold;\r\n    color: black;\r\n  }\r\n  \r\n  .v-btn:hover {\r\n    background-color: darkgoldenrod;\r\n  }\r\n  \r\n  .v-dialog .v-card {\r\n    background-color: black;\r\n    color: white;\r\n  }\r\n  \r\n  .v-dialog .v-btn {\r\n    color: gold;\r\n  }\r\n  </style>\r\n  "],"mappings":";AA0CE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,CACR;QACEC,IAAI,EAAE,SAAS;QACfC,WAAW,EAAE;MACf,CAAC,EACD;QACED,IAAI,EAAE,QAAQ;QACdC,WAAW,EAAE;MACf,CAAC,EACD;QACED,IAAI,EAAE,SAAS;QACfC,WAAW,EAAE;MACf;MACA;MAAA,C