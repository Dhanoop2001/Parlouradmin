"password\"\r\n            placeholder=\"Enter your password\"\r\n            v-model=\"password\"\r\n            @blur=\"validatePassword\"\r\n            required\r\n          />\r\n          <button\r\n            type=\"button\"\r\n            class=\"password-toggle\"\r\n            @click=\"togglePasswordVisibility\"\r\n          >\r\n            {{ isPasswordVisible ? 'Hide' : 'Show' }}\r\n          </button>\r\n        </div>\r\n        <small v-if=\"errors.password\" class=\"error-message\">{{ errors.password }}</small>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"gender\">Gender</label>\r\n        <select id=\"gender\" v-model=\"gender\" required>\r\n          <option value=\"\" disabled>Select your gender</option>\r\n          <option value=\"male\">Male</option>\r\n          <option value=\"female\">Female</option>\r\n        </select>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <button\r\n          type=\"submit\"\r\n          class=\"signup-button\"\r\n          :disabled=\"isSubmitting\"\r\n        >\r\n          {{ isSubmitting ? 'Submitting...' : 'Sign Up' }}\r\n        </button>\r\n      </div>\r\n      <div class=\"form-group already-account\">\r\n        <span>Already have an account? <router-link to=\"/log-in\">Login</router-link></span>\r\n      </div>\r\n    </form>\r\n\r\n    <!-- Registration Success Popup -->\r\n    <div v-if=\"showSuccessPopup\" class=\"success-popup\">\r\n      <p>Registration Successful!</p>\r\n      <button @click=\"closePopup\">OK</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"SignUpPage\",\r\n  data() {\r\n    return {\r\n      isPasswordVisible: false,\r\n      name: \"\",\r\n      email: \"\",\r\n      password: \"\",\r\n      gender: \"\",\r\n      errors: {\r\n        name: \"\",\r\n        password: \"\",\r\n      },\r\n      showSuccessPopup: false,\r\n      isSubmitting: false,\r\n    };\r\n  },\r\n  methods: {\r\n    validateName() {\r\n      const namePattern = /^[A-Za-z\\s]+$/;\r\n      if (!this.name) {\r\n        this.errors.name = \"Name is required.\";\r\n      } else if (!namePattern.test(this.name)) {\r\n        this.errors.name = \"Name must contain only alphabets.\";\r\n      } else {\r\n        this.errors.name = \"\";\r\n      }\r\n    },\r\n    validatePassword() {\r\n      const passwordPattern = /^(?=.*[A-Za-z])(?=.*\\d)[A-Za-z\\d]{8,}$/;\r\n      if (!this.password) {\r\n        this.errors.password = \"Password is required.\";\r\n      } else if (!passwordPattern.test(this.password)) {\r\n        this.errors.password =\r\n          \"Password must contain at least 8 characters, including both letters and numbers.\";\r\n      } else {\r\n        this.errors.password = \"\";\r\n      }\r\n    },\r\n    togglePasswordVisibility() {\r\n      this.isPasswordVisible = !this.isPasswordVisible;\r\n    },\r\n    async handleSignUp() {\r\n      this.validateName();\r\n      this.validatePassword();\r\n\r\n      if (!this.errors.name && !this.errors.password && this.email && this.gender) {\r\n        this.isSubmitting = true;\r\n\r\n        try {\r\n          /* \r\n          // Uncomment this block when backend is available\r\n          const response = await axios.post(\"http://localhost:8080/api/register\", {\r\n            name: this.name,\r\n            email: this.email,\r\n            password: this.password,\r\n            gender: this.gender,\r\n          });\r\n\r\n          if (response.status === 200) {\r\n            this.showSuccessPopup = true;\r\n          }\r\n          */\r\n\r\n          // Simulated success response\r\n          console.log(\"Simulating successful registration.\");\r\n          this.showSuccessPopup = true;\r\n        } catch (error) {\r\n          console.error(\"Registration failed\", error);\r\n          alert(\"An error occurred during registration. Please try again.\");\r\n        } finally {\r\n          this.isSubmitting = false;\r\n        }\r\n      } else {\r\n        alert(\"Please fill out all fields correctly.\");\r\n      }\r\n    },\r\n    closePopup() {\r\n      this.showSuccessPopup = false;\r\n      this.$router.push(\"/log-in\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.signup-container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  height: 100vh;\r\n  background-image: url('@/assets/goldbg.jpg');\r\n  background-size: cover;\r\n  background-position: center;\r\n  background-repeat: no-repeat;\r\n}\r\n\r\n.signup-heading {\r\n  font-size: 3rem;\r\n  color: #fff;\r\n  margin-bottom: 1.5rem;\r\n  text-shadow: 2px 2px 10px rgba(0, 0, 0, 0.7);\r\n  font-family: \"Arial\", sans-serif;\r\n}\r\n\r\n.signup-form {\r\n  padding: 2.5rem;\r\n  border-radius: 12px;\r\n  box-shadow: 10px 10px 10px rgba(0, 0, 0, 0.3);\r\n  width: 90%;\r\n  max-width: 450px;\r\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\r\n  border: solid 1px gold;\r\n}\r\n\r\n.signup-form:hover {\r\n  box-shadow: 0 15px 25px rgba(0, 0, 0, 0.5);\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 1.5rem;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  margin-bottom: 0.75rem;\r\n  font-weight: 600;\r\n  color: #ddd;\r\n  font-size: 1.1rem;\r\n  font-family: \"Arial\", sans-serif;\r\n}\r\n\r\ninput, select {\r\n  width: 100%;\r\n  padding: 1rem;\r\n  border: none;\r\n  border-radius: 30px;\r\n  font-size: 1rem;\r\n  background-color: black;\r\n  color: #fff;\r\n  box-sizing: border-box;\r\n  transition: background-color 0.3s ease, border 0.3s ease;\r\n}\r\n\r\ninput:focus, select:focus {\r\n  background-color: black;\r\n  outline: none;\r\n  border: 2px solid gold;\r\n  border-radius: 30px;\r\n}\r\n\r\n.password-container {\r\n  position: relative;\r\n}\r\n\r\n.password-toggle {\r\n  position: absolute;\r\n  right: 10px;\r\n  top: 50%;\r\n  transform: translateY(-50%);\r\n  background: none;\r\n  border: none;\r\n  color: gold;\r\n  font-size: 0.8rem;\r\n  cursor: pointer;\r\n  text-decoration: underline;\r\n}\r\n\r\n.signup-button {\r\n  width: 100%;\r\n  padding: 1rem;\r\n  background-color: black;\r\n  color: #fff;\r\n  border: 1px solid gold;\r\n  border-radius: 30px;\r\n  font-size: 1.1rem;\r\n  font-weight: bold;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s, transform 0.2s ease-in-out;\r\n}\r\n\r\n.signup-button:hover {\r\n  background-color: black;\r\n  transform: translateY(-2px);\r\n  border: 1px solid gold;\r\n}\r\n\r\n.signup-button:active {\r\n  background-color: gold;\r\n  transform: translateY(1px);\r\n}\r\n\r\n.signup-button:disabled {\r\n  background-color: #9e9e9e;\r\n  cursor: not-allowed;\r\n}\r\n\r\n.already-account {\r\n  text-align: center;\r\n  font-size: 1rem;\r\n  color: white;\r\n  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.8);\r\n}\r\n\r\n.already-account a {\r\n  color: blue;\r\n  text-decoration: none;\r\n  font-weight: bold;\r\n}\r\n\r\n.already-account a:hover {\r\n  text-decoration: underline;\r\n}\r\n\r\n.success-popup {\r\n  position: fixed;\r\n  height: 200px;\r\n  width: 400px;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  background: black;\r\n  padding: 2rem;\r\n  border-radius: 12px;\r\n  text-align: center;\r\n  z-index: 1000;\r\n  border: 2px solid gold;\r\n}\r\n\r\n.success-popup p {\r\n  font-size: 1.2rem;\r\n  color: white;\r\n  margin-bottom: 2rem;\r\n  font-weight: bold;\r\n}\r\n\r\n.success-popup button {\r\n  padding: 0.7rem 1.5rem;\r\n  background-color: gold;\r\n  color: black;\r\n  border: none;\r\n  border-radius: 12px;\r\n  font-size: 1rem;\r\n  font-weight: bold;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease, transform 0.2s ease-in-out;\r\n}\r\n\r\n.success-popup button:hover {\r\n  background-color: #ffd700; /* Slightly lighter gold */\r\n  transform: translateY(-2px);\r\n}\r\n\r\n.success-popup button:active {\r\n  background-color: #daa520; /* Darker gold */\r\n  transform: translateY(1px);\r\n}\r\n\r\n.error-message {\r\n  color: #ff4d4f;\r\n  font-size: 0.9rem;\r\n  margin-top: 0.5rem;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAkB;;EAGpBA,KAAK,EAAC;AAAY;;EAJ7BC,GAAA;EAckCD,KAAK,EAAC;;;EAE7BA,KAAK,EAAC;AAAY;;EAUlBA,KAAK,EAAC;AAA2B;;EAE/BA,KAAK,EAAC;AAAoB;mBA5BvC;;EAAAC,GAAA;EA6CsCD,KAAK,EAAC;;;EAEjCA,KAAK,EAAC;AAAY;;EAQlBA,KAAK,EAAC;AAAY;oBAvD7B;;EAgEWA,KAAK,EAAC;AAA4B;;EAhE7CC,GAAA;EAsEiCD,KAAK,EAAC;;;;uBArErCE,mBAAA,CAyEM,OAzENC,UAyEM,G,4BAxEJC,mBAAA,CAAuC;IAAnCJ,KAAK,EAAC;EAAgB,GAAC,SAAO,sBAClCI,mBAAA,CAgEO;IAhEDJ,KAAK,EAAC,aAAa;IAAEK,QAAM,EAAAC,MAAA,QAAAA,MAAA,MAHrCC,cAAA,KAAAC,IAAA,KAG+CC,QAAA,CAAAC,YAAA,IAAAD,QAAA,CAAAC,YAAA,IAAAF,IAAA,CAAY;MACrDJ,mBAAA,CAWM,OAXNO,UAWM,G,0BAVJP,mBAAA,CAA8B;IAAvBQ,GAAG,EAAC;EAAM,GAAC,MAAI,sB,gBACtBR,mBAAA,CAOE;IANAS,IAAI,EAAC,MAAM;IACXC,EAAE,EAAC,MAAM;IACTC,WAAW,EAAC,iBAAiB;IATvC,uBAAAT,MAAA,QAAAA,MAAA,MAAAU,MAAA,IAUmBC,KAAA,CAAAC,IAAI,GAAAF,MAAA;IACZG,MAAI,EAAAb,MAAA,QAAAA,MAAA,UAAAE,IAAA,KAAEC,QAAA,CAAAW,YAAA,IAAAX,QAAA,CAAAW,YAAA,IAAAZ,IAAA,CAAY;IACnBa,QAAQ,EAAR;iEAFSJ,KAAA,CAAAC,IAAI,E,GAIFD,KAAA,CAAAK,MAAM,CAACJ,IAAI,I,cAAxBhB,mBAAA,CAAyE,SAAzEqB,UAAyE,EAAAC,gBAAA,CAAtBP,KAAA,CAAAK,MAAM,CAACJ,IAAI,oBAdtEO,mBAAA,e,GAgBMrB,mBAAA,CASM,OATNsB,UASM,G,4BARJtB,mBAAA,CAAgC;IAAzBQ,GAAG,EAAC;EAAO,GAAC,OAAK,sB,gBACxBR,mBAAA,CAME;IALAS,IAAI,EAAC,OAAO;IACZC,EAAE,EAAC,OAAO;IACVC,WAAW,EAAC,kBAAkB;IArBxC,uBAAAT,MAAA,QAAAA,MAAA,MAAAU,MAAA,IAsBmBC,KAAA,CAAAU,KAAK,GAAAX,MAAA;IACdK,QAAQ,EAAR;iDADSJ,KAAA,CAAAU,KAAK,E,KAIlBvB,mBAAA,CAoBM,OApBNwB,UAoBM,G,4BAnBJxB,mBAAA,CAAsC;IAA/BQ,GAAG,EAAC;EAAU,GAAC,UAAQ,sBAC9BR,mBAAA,CAgBM,OAhBNyB,UAgBM,G,gBAfJzB,mBAAA,CAOE;IANCS,IAAI,EAAEI,KAAA,CAAAa,iBAAiB;IACxBhB,EAAE,EAAC,UAAU;IACbC,WAAW,EAAC,qBAAqB;IAhC7C,uBAAAT,MAAA,QAAAA,MAAA,MAAAU,MAAA,IAiCqBC,KAAA,CAAAc,QAAQ,GAAAf,MAAA;IAChBG,MAAI,EAAAb,MAAA,QAAAA,MAAA,UAAAE,IAAA,KAAEC,QAAA,CAAAuB,gBAAA,IAAAvB,QAAA,CAAAuB,gBAAA,IAAAxB,IAAA,CAAgB;IACvBa,QAAQ,EAAR;2CAnCZY,UAAA,I,iBAiCqBhB,KAAA,CAAAc,QAAQ,E,GAInB3B,mBAAA,CA