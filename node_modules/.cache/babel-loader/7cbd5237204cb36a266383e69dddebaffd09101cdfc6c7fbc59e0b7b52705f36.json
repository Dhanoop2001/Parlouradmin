{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"SignUpPage\",\n  data() {\n    return {\n      isPasswordVisible: false,\n      name: \"\",\n      email: \"\",\n      password: \"\",\n      gender: \"\",\n      errors: {\n        name: \"\",\n        email: \"\",\n        password: \"\",\n        gender: \"\"\n      },\n      showSuccessPopup: false,\n      isSubmitting: false\n    };\n  },\n  computed: {\n    isFormValid() {\n      return !this.errors.name && !this.errors.email && !this.errors.password && !this.errors.gender && this.name && this.email && this.password && this.gender;\n    }\n  },\n  methods: {\n    validateName() {\n      const namePattern = /^[A-Za-z\\s]+$/;\n      if (!this.name) {\n        this.errors.name = \"Name is required.\";\n      } else if (!namePattern.test(this.name)) {\n        this.errors.name = \"Name must contain only alphabets.\";\n      } else {\n        this.errors.name = \"\";\n      }\n    },\n    validateEmail() {\n      const emailPattern = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/;\n      this.errors.email = emailPattern.test(this.email) ? \"\" : \"Enter a valid email address.\";\n    },\n    validatePassword() {\n      this.errors.password = this.password.length >= 6 ? \"\" : \"Password must be at least 6 characters long.\";\n    },\n    validateGender() {\n      this.errors.gender = this.gender ? \"\" : \"Please select a gender.\";\n    },\n    togglePasswordVisibility() {\n      this.isPasswordVisible = !this.isPasswordVisible;\n    },\n    handleSignUp() {\n      if (this.isFormValid) {\n        this.isSubmitting = true;\n        setTimeout(() => {\n          this.showSuccessPopup = true;\n          this.isSubmitting = false;\n        }, 2000);\n      } else {\n        alert(\"Please fill all fields correctly.\");\n      }\n    },\n    closePopup() {\n      this.showSuccessPopup = false;\n      this.$router.push(\"/log-in\");\n    }\n  }\n};","map":{"version":3,"names":["name","data","isPasswordVisible","email","password","gender","errors","showSuccessPopup","isSubmitting","computed","isFormValid","methods","validateName","namePattern","test","validateEmail","emailPattern","validatePassword","length","validateGender","togglePasswordVisibility","handleSignUp","setTimeout","alert","closePopup","$router","push"],"sources":["C:\\Users\\HP-15\\Desktop\\parlouradmin\\parlouradmin\\src\\components\\SignUp.vue"],"sourcesContent":["<template>\r\n  <div class=\"signup-container\">\r\n    <h2 class=\"signup-heading\">Sign Up</h2>\r\n    <form class=\"signup-form\" @submit.prevent=\"handleSignUp\">\r\n      <div class=\"form-group\">\r\n        <label for=\"name\">Name</label>\r\n        <input\r\n          type=\"text\"\r\n          id=\"name\"\r\n          placeholder=\"Enter your name\"\r\n          v-model=\"name\"\r\n          @blur=\"validateName\"\r\n          required\r\n        />\r\n        <small v-if=\"errors.name\" class=\"error-message\">{{ errors.name }}</small>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"email\">Email</label>\r\n        <input\r\n          type=\"email\"\r\n          id=\"email\"\r\n          placeholder=\"Enter your email\"\r\n          v-model=\"email\"\r\n          @blur=\"validateEmail\"\r\n          required\r\n        />\r\n        <small v-if=\"errors.email\" class=\"error-message\">{{ errors.email }}</small>\r\n      </div>\r\n      <div class=\"form-group password-group\">\r\n        <label for=\"password\">Password</label>\r\n        <div class=\"password-container\">\r\n          <input\r\n            :type=\"isPasswordVisible ? 'text' : 'password'\"\r\n            id=\"password\"\r\n            placeholder=\"Enter your password\"\r\n            v-model=\"password\"\r\n            @blur=\"validatePassword\"\r\n            required\r\n          />\r\n          <button\r\n            type=\"button\"\r\n            class=\"password-toggle\"\r\n            @click=\"togglePasswordVisibility\"\r\n          >\r\n            {{ isPasswordVisible ? 'Hide' : 'Show' }}\r\n          </button>\r\n        </div>\r\n        <small v-if=\"errors.password\" class=\"error-message\">{{ errors.password }}</small>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"gender\">Gender</label>\r\n        <select id=\"gender\" v-model=\"gender\" @blur=\"validateGender\" required>\r\n          <option value=\"\" disabled>Select your gender</option>\r\n          <option value=\"male\">Male</option>\r\n          <option value=\"female\">Female</option>\r\n        </select>\r\n        <small v-if=\"errors.gender\" class=\"error-message\">{{ errors.gender }}</small>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <button\r\n          type=\"submit\"\r\n          class=\"signup-button\"\r\n          :disabled=\"isSubmitting || !isFormValid\"\r\n        >\r\n          {{ isSubmitting ? 'Submitting...' : 'Sign Up' }}\r\n        </button>\r\n      </div>\r\n      <div class=\"form-group already-account\">\r\n        <span>Already have an account? <router-link to=\"/log-in\">Login</router-link></span>\r\n      </div>\r\n    </form>\r\n\r\n    <div v-if=\"showSuccessPopup\" class=\"success-popup\">\r\n      <p>Registration Successful!</p>\r\n      <button @click=\"closePopup\">OK</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"SignUpPage\",\r\n  data() {\r\n    return {\r\n      isPasswordVisible: false,\r\n      name: \"\",\r\n      email: \"\",\r\n      password: \"\",\r\n      gender: \"\",\r\n      errors: {\r\n        name: \"\",\r\n        email: \"\",\r\n        password: \"\",\r\n        gender: \"\",\r\n      },\r\n      showSuccessPopup: false,\r\n      isSubmitting: false,\r\n    };\r\n  },\r\n  computed: {\r\n    isFormValid() {\r\n      return (\r\n        !this.errors.name &&\r\n        !this.errors.email &&\r\n        !this.errors.password &&\r\n        !this.errors.gender &&\r\n        this.name &&\r\n        this.email &&\r\n        this.password &&\r\n        this.gender\r\n      );\r\n    },\r\n  },\r\n  methods: {\r\n    validateName() {\r\n      const namePattern = /^[A-Za-z\\s]+$/;\r\n      if (!this.name) {\r\n        this.errors.name = \"Name is required.\";\r\n      } else if (!namePattern.test(this.name)) {\r\n        this.errors.name = \"Name must contain only alphabets.\";\r\n      } else {\r\n        this.errors.name = \"\";\r\n      }\r\n    },\r\n    validateEmail() {\r\n      const emailPattern = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/;\r\n      this.errors.email = emailPattern.test(this.email)\r\n        ? \"\"\r\n        : \"Enter a valid email address.\";\r\n    },\r\n    validatePassword() {\r\n      this.errors.password =\r\n        this.password.length >= 6\r\n          ? \"\"\r\n          : \"Password must be at least 6 characters long.\";\r\n    },\r\n    validateGender() {\r\n      this.errors.gender = this.gender ? \"\" : \"Please select a gender.\";\r\n    },\r\n    togglePasswordVisibility() {\r\n      this.isPasswordVisible = !this.isPasswordVisible;\r\n    },\r\n    handleSignUp() {\r\n      if (this.isFormValid) {\r\n        this.isSubmitting = true;\r\n        setTimeout(() => {\r\n          this.showSuccessPopup = true;\r\n          this.isSubmitting = false;\r\n        }, 2000);\r\n      } else {\r\n        alert(\"Please fill all fields correctly.\");\r\n      }\r\n    },\r\n    closePopup() {\r\n      this.showSuccessPopup = false;\r\n      this.$router.push(\"/log-in\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.signup-container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  height: 100vh;\r\n  background: url('@/assets/salon.jpg') center/cover no-repeat;\r\n}\r\n\r\n.signup-heading {\r\n  font-size: 2.5rem;\r\n  color: white;\r\n  margin-bottom: 1.5rem;\r\n  text-shadow: 2px 2px 10px rgba(0, 0, 0, 0.7);\r\n}\r\n\r\n.signup-form {\r\n  background: #333;\r\n  padding: 2rem;\r\n  border-radius: 10px;\r\n  width: 90%;\r\n  max-width: 400px;\r\n  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.5);\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 1.5rem;\r\n}\r\n\r\nlabel {\r\n  font-weight: bold;\r\n  color: #fff;\r\n  margin-bottom: 0.5rem;\r\n  display: block;\r\n}\r\n\r\ninput, select {\r\n  width: 100%;\r\n  padding: 0.8rem;\r\n  border-radius: 8px;\r\n  background: #444;\r\n  color: white;\r\n  border: none;\r\n}\r\n\r\ninput:focus, select:focus {\r\n  outline: 2px solid #66bb6a;\r\n}\r\n\r\n.password-container {\r\n  position: relative;\r\n}\r\n\r\n.password-toggle {\r\n  position: absolute;\r\n  right: 10px;\r\n  top: 50%;\r\n  transform: translateY(-50%);\r\n  background: none;\r\n  color: #66bb6a;\r\n  border: none;\r\n  cursor: pointer;\r\n}\r\n\r\n.signup-button {\r\n  background: #66bb6a;\r\n  color: white;\r\n  font-size: 1rem;\r\n  font-weight: bold;\r\n  padding: 0.8rem;\r\n  border-radius: 8px;\r\n  border: none;\r\n  cursor: pointer;\r\n}\r\n\r\n.signup-button:hover {\r\n  background: #4caf50;\r\n}\r\n\r\n.error-message {\r\n  color: #ff4d4f;\r\n  font-size: 0.9rem;\r\n  margin-top: 0.5rem;\r\n}\r\n\r\n.success-popup {\r\n  position: fixed;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  background: white;\r\n  padding: 2rem;\r\n  border-radius: 10px;\r\n  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.5);\r\n}\r\n\r\n.success-popup button {\r\n  background: #66bb6a;\r\n  color: white;\r\n  padding: 0.5rem 1.5rem;\r\n  border: none;\r\n  border-radius: 5px;\r\n}\r\n</style>\r\n"],"mappings":";AAgFA,eAAe;EACbA,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,iBAAiB,EAAE,KAAK;MACxBF,IAAI,EAAE,EAAE;MACRG,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE,EAAE;MACVC,MAAM,EAAE;QACNN,IAAI,EAAE,EAAE;QACRG,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,MAAM,EAAE;MACV,CAAC;MACDE,gBAAgB,EAAE,KAAK;MACvBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAWA,CAAA,EAAG